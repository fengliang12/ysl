.Land_Content .Land_form input:last-of-type{
	width:40%;
	height: 35px;
	text-align: center;
	line-height: 35px;
	color: white;
	background: black;
	position: relative;
	right:0;
	margin-left:8%;
}
.line{
	margin-top: 39px;
	margin-bottom: 20px;
	width: 100%;
	height: 1px;
	background: #dbdbdb;
	position: relative;
}
.line .line_moddle{
	position: absolute;
	display: inline-block;
	top: -8px;
	left: 50%;
	transform: translate(-50%);
	width: 50px;
	background: white;
	text-align: center;
	font-size: 12px;
}
 .other_land{
 	width: 100%;
 }
 .other_land ul{
 	overflow: hidden;
 	margin: 0 auto;
 	width: 100%;
	margin-top: 20px;
}
.other_land a{
	color: black;
}
 .other_land ul li{
	float: left;
	margin-right:8%;
	margin-bottom:56px;
	width: 40px;
	height:40px;
	border: 1px solid #ebebeb;
	padding:6px;
}
 .other_land ul li a{
	font-size: 12px;
	display: inline-block;
	text-decoration: none;
	background-size: 354px 354px;
	text-align: center;
}
 .other_land ul li:first-child a{
	background-position:-331px 0 ;
	width: 23px;
	height: 27px;
}
 .other_land ul li:nth-child(2) a{
	background-position:-35px -244px ;
	width: 29px;
	height: 26px;
}
 .other_land ul li:nth-child(3) a{
	background-position:-28px -279px ;
	width: 25px;
	height: 25px;
}
 .other_land ul li:nth-child(4) a{
	background-position:-70px -244px ;
	width: 29px;
	height: 23px;
}
 .other_land ul li a{
	display: block;
	margin: 0 auto;
	background-image:url(../img/icon.png);
	background-size: 354px 354px;
}
 .other_land ul li a span{
	width: 40px;
	height: 14px;
	display: block;
	position: relative;
	top: 40px;
	left: -7px;
	font-size: 12px;
}

