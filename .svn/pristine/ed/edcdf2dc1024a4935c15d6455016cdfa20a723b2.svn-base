*{
	user-select:none;
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}
a{
	color: black;
}
form input{
	border: 1px solid #dadada;
}
ul{
	list-style: none;
}
.red{
	color: red;
}
/*登陆页面*/

.Land_Content .Land_form p span{
	color: red;
}
.Land_Content{
	padding: 25px 25px 0 25px;
	width: 100%;
	margin: 0 auto;
	min-width: 320px;
}
.Land_Content .Land_form{
	margin: 0 auto;
	width: 100%;
}
.Land_Content .Land_form a{
	color: black;
	font-size: 12px;
}
.Land_Content .Land_form a:first-of-type{
	position: relative;
	right:-80px;

}
.Land_Content .Land_form span:last-of-type{
	font-size: 14px;			
}
.Land_Content .Land_form a:last-of-type{
	text-decoration: none;
	display: inline-block;
	border: 1px solid black;
	width:40%;
	height: 33px;
	padding: 0 20px;
	text-align: center;
	line-height: 35px;
	position: relative;
	top: 1px;
	right: 0;
}
.Land_Content .Land_form input,p{
	margin-top: 15px;
	font-size: 12px;
}
.Land_Content .Land_form input:nth-last-of-type(n+3){
	width: 88%;
	height: 40px;
	padding: 5px;
}
.Land_Content .Land_form input:nth-of-type(3){
	position: relative;
	top: 3px;
	margin-left: 5px;
	width: 17px;
	height: 17px;
	/*visibility: hidden;*/
}
