<script id="template" type="text/html">
{{if data}}
        {{each data as value}}
                <div class="shopcart_list_items">
                            <div class="items_left">
                            <div class="items_image">
                            <input type="checkbox" class="aloneButton" data-price="{{value.cartPrice}}" data-id="{{value.cartId}}">
                            <img src="{{value.cartImg}}" alt="">
                            </div>
                            </div>
                            <div class="items_right">
                            <div class="items_description">
                            {{value.cartDescription}}
                    </div>
                    <select name="" data-count="{{value.cartCount}}" data-id="{{value.cartId}}">
                            <option value="1">1</option>
                            <option value="2">2</option>
                            <option value="3">3</option>
                    </select>
                            <label for="">￥{{value.cartPrice}}</label>
                            <ul>
                                <li class="update" data-value="{{value.cartId}}">修改</li>
                                <li>心愿单</li>
                                <li class="delete" data-value="{{value.cartId}}">删除</li>
                            </ul>
                            </div>
                </div>
        {{/each}}
{{/if}}
</script>
