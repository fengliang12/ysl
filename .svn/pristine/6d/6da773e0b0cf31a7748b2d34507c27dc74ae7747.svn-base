.wrapper{
    width: 100%;
    max-width: 640px;

}
header{
    width: 100%;
    max-width: 640px;
    height: 64px;
    position: relative;
    border-bottom: 1px solid #cccccc;
}
header img{
    width: 50%;
    height: 50%;
    display: inline-block;
    position: absolute;
    left: 50%;
    top:50%;
    transform: translate(-50%,-50%);

}
section{
    width: 100%;
    border-bottom: 2px solid #000;
}
section .go_shopping{
    width: 90%;
    margin: 0 auto;
    height: 50px;
    line-height: 50px;
    font-size: 14px;
    font-weight: 700;
    color: #000000;
}
section .go_shopping span{
    margin-right: 3px;
}
section .cart-content{
    width: 90%;
    margin: 0 auto;
    /*display: none;*/
}
section .cart-content .shopping-title{
    width: 100%;
    text-align: center;
    padding:20% 30%;
    border: 1px solid gray;
}
section .cart-content .shopping-title span{
    display: block;
    font:500 14px 微软雅黑;
    color:gray;
}
section .cart-content .shoppingcart-empty{
    width: 100%;
}
section .cart-content .shoppingcart-empty .float-clearfix{
    width: 100%;
}
section .cart-content .shoppingcart-empty .float-clearfix li{
    width: 100%;
    text-align: left;
    padding: 27px 18px;
    background-color: #eeeeee;
    margin-top: 20px;
    position: relative;

}
section .cart-content .shoppingcart-empty .float-clearfix li h3{
    font-size: 20px;
    color: #000;
}
section .cart-content .shoppingcart-empty .float-clearfix li p{
    font-size: 14px;
    color: #000;
}
section .cart-content .shoppingcart-empty .float-clearfix li i{
    position: absolute;
    font-size: 40px;
    right: 10px;
    top: 50%;
    color: #000;
    transform: translate(0,-50%);
}
section .safe-box{
    width: 100%;
    margin: 0 auto;
    margin-top: 50px;
    padding:50px 20px;
    background-color: #eeeeee;
    text-align: center;
}
section .safe-box p{
    margin:10px 0;
}
section .safe-box .safe-ensure{
    width: 100%;
}
section .safe-box .safe-ensure img{
    width: 50%;
}



section .shopcart{
    width: 100%;
    margin: 0 auto;
    padding-bottom: 20px;
    display: none;
    border: 1px solid #000000;
}
section .shopcart .shopcart_list_title{
    height: 90px;
    font-size: 25px;
    line-height: 50px;
}
section .shopcart .shopcart_list_title p{
    width: 100%;
    height: 50px;
    font-size: 25px;
    line-height: 50px;
    padding-left: 20px;

}
section .shopcart .shopcart_list_title_down {
    width: 100%;
    height: 40px;
    background-color: #000;
    line-height: 40px;
    color: #fff;
    font-size: 20px;
    padding-left: 20px;
}
section .shopcart .shopcart_list_title_down input{
    width: 18px;
    height: 18px;
    vertical-align: middle;
}
section .shopcart .shopcart_list_items{
    width: 90%;
    margin: 20px auto;
    overflow: hidden;
    border-bottom: 1px solid gray;

}

section .shopcart .items_image{
    position: relative;
}
section .shopcart .items_image input{
    position: absolute;
    bottom: -110px;
    left: 0px;
    width: 20px;
    height: 20px;
}
section .shopcart .items_image img{
    width: 80%;
    float: right;
}
section .shopcart .items_description{
    font-size: 16px;
    line-height: 14px;
    margin-bottom: 20px;
}
section .shopcart .items_right select{
    width: 50px;
    height: 30px;
    vertical-align: middle;
}
section .shopcart .items_right label{
   float: right;
    margin-right: 20px;
    font-size: 16px;
}
section .shopcart .items_right ul{
   list-style: none;
    margin-top: 20px;
}
section .shopcart .items_right ul li{
   float: left;
    margin-left: 10px;
}
section .shopcart .items_left{
    float: left;
    width: 45%;
}
section .shopcart .items_right{
    float: right;
    width: 55%;
}

section .shopcart .shoppingcart-use-box{
    width: 90%;
    margin: 20px auto;
    /*height: 200px;*/
    border-bottom: 1px solid gray;
    padding-bottom: 20px;

}
section .shopcart .shoppingcart-use-box .shopping-use-content{
    width: 100%;

}
section .shopcart  .shopping-use-content .titles{
    font-size: 18px;
}
section .shopcart  .shopping-use-content .titles{
    font-size: 18px;
}

section .shopcart .shoppingcart-use-box .orderprice-box p{
  margin-top: 5px;

}
section .shopcart .shoppingcart-use-box .orderprice-box p:last-child{
    font:700 16px 微软雅黑;
    margin-top: 20px;
}
section .shopcart .shoppingcart-use-box .orderprice-box span{
   float: right;

}
.cut_img{
    width: 100%;
    height: 100px;
}
.cut_img img{
    width: 100%;
    height: 100%;
}






footer{
    width: 90%;
    margin: 0 auto;

}
footer .footer_title{
    width: 100%;
    padding: 20px 0;
    height: 80px;

}
footer .footer_title p{
    width: 60%;
    text-align: center;
    float: left;
    margin-left: 5px;
}
footer .footer_title a{
    float: right;
    width: 30%;
    height: 30px;
    margin-right: 5px;
    line-height: 30px;
    text-align: center;
    border: 1px solid gray;
    color: #000000;
}


footer .footer_logo{
    width: 100%;
    padding: 0px;
    height: 80px;
    position: relative;

}
footer .footer_logo a{
    display: inline-block;
    width: 60%;
    position: absolute;
    left: 50%;
    transform: translate(-50%,0%);

}
footer .footer_logo a img{
    width: 100%;
}
footer .footer_please{
    width: 100%;
}
footer .footer_please a{
    display: inline-block;
    width: 30.33%;
    font-size: 50px;
    color: #000000;
    text-align: center;
}
footer .footer_please span{
    display: inline-block;
    width: 32.33%;
    text-align: left;
    color: #000000;
}
footer .footer-bottom{
    width: 100%;
    margin: 0 auto;
    height: 200px;
    background-color: #eee;
    text-align: center;
    padding: 20px;
}
footer .footer-bottom .statement{
    width: 100%;
    margin: 0 auto;
    text-align: center;
}
footer .footer-bottom .statement li{
    width: 100%;
    margin-top: 5px;
}
footer .footer-bottom .statement li:last-child span{
    font-size: 8px;
}
footer .footer-bottom .con-content{
    width:100%;
}
footer .footer-bottom .con-content ul{
    margin-top: 10px;
    width:100%;
}
footer .footer-bottom .con-content ul li a{
    float: left;
    color: #000000;
    margin-left: 10px;
}

.shopping_buy{
    position: fixed;
    bottom: 0;
    left: 0;
    width:100%;
    height:50px;
    background-color: #000;
}
.shopping_buy .totle_price{
    position: fixed;
    bottom: 0;
    left: 0;
    width:100%;
    height:50px;
    color: #fff;
    line-height: 50px;
}
.shopping_buy p{
    float: left;
    margin-left: 10px;
    font-size: 14px;
}
.shopping_buy .continue_shopping{
    float: right;
    width:80px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    color: #fff;
    margin:10px 5px;
    border: 1px solid #fff;
}
.shopping_buy .none_shopping{
    float: right;
    width:80px;
    height: 30px;
    text-align: center;
    line-height: 30px;
    color: #000;
    margin:10px 5px;
    background-color: #ffffff;
}
.shopping_buy .phone{
    float: right;
    width:20px;
    height: 20px;
    margin:10px 5px;
    font-size: 25px;
    color: #fff;
    margin-right: 10px;
}



