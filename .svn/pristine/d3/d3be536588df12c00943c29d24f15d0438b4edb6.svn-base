*{
    padding: 0;
    margin:0;
    box-sizing: border-box;
    word-wrap: break-word;
    outline: 0;
}
body{
    font-family: "Helvetica","PingFangSC-Regular","Arial","Times New Roman","微软雅黑","Microsoft YaHei",sans-serif;
    color: #000;
    font-size: 16px;
    letter-spacing: 0;
    width: 100%;
    line-height: 1.2;
    overflow-x: hidden;
    height:auto;
}
.clearfix::before,.clearfix::after{
    display: block;
    content: "";
    line-height: 0px;
    height: 0px;
    clear: both;
    visibility: hidden;
}
.layout{
    width: 100%;
   /* border:1px solid red;*/
    max-width:640px;
    margin:0 auto;
}
.header{
    width:100%;
    position: fixed;
    z-index:9;
    max-width: 640px;
}
.top{
    width:100%;
    text-align: center;
    height:55px;
    background-color: black;
    color: #fff;
    max-width: 640px;
}
.top .container{
    margin:0 auto;
    width:100%;
    padding: 10px 20px;
    font-size: 0.8em;
}
.top .container a{
    color: #fff;
}
.top i{
    color: #fff;
    position: absolute;
    top:2px;
    right:2px;
    width:20px;
    height:20px;
    cursor: pointer;
    font-weight: inherit;
    font-size: 0.8em;
}
.nav{
    width:100%;
    background-color: white;
    position: relative;
    z-index:9;
    padding:0;
    max-width: 640px;
}
.nav .content{
    width:100%;
    padding:0 24px;
    height:50px;
}
.nav .content a{
    display: inline-block;
    line-height: 50px;
    position: relative;
    left: -10px;
    padding: 0 10px;
    color: #000;
}
.nav .content a .bars{
    background-position: -305px -165px;
    display: inline-block;
    width: 19px;
    height: 14px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
}
.nav .content a .user{
    background-position: -241px -115px;
    width: 15px;
    height: 17px;
    display: inline-block;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
}

.nav .content .logo{
    display: block;
    width: 138px;
    position: absolute;
    top: 0px;
    left: 50%;
    margin-left: -69px;
}
.nav .content .logo img{
    width:100%;
    height:23px;
    position: relative;
    top:5px;
}
.nav .content .right{
    position: absolute;
    right:10px;
    top:0;
}
.nav .content .right .search{
    background-position: -305px -255px;
    width: 16px;
    height: 16px;
    display: inline-block;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    position: relative;
    right:5px;
}
.nav .content .right .bag{
    display: inline-block;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    background-position: -51px -337px;
    width: 11px;
    height: 16px;
    top:-1px;
    position: relative;
}
.nav .content .searchbox{
    width:100%;
    position: absolute;
    left:0;
    top:50px;
    background-color: white;
    height:40px;
    z-index: 9;
    color: #000;
    display: none;
}
.fadeIn {
    animation-name: fadeIn;
}
.aniE1 {
    animation-delay: 0.5s;
}
.nav .content .searchbox input{
    border:0;
    font-size: 12px;
    width: 100%;
    height: 39px;
    padding: 5px 38px 5px 9px;
    font-family: "微软雅黑";
}
.nav .content .searchbox a{
    display: inline-block;
    line-height: 38px;
    width:38px;
    height:38px;
    position: absolute;
    top: 5%;
    left: 100%;
    transform: translateX(-100%);
}
.min-nav{
    padding: 0 20px;
    background: #f2f2f2;
    margin-top: 0;
    height:40px;
    width:100%;
}
.min-nav .wrap{
    overflow: hidden;
    max-width:100%;
    position: relative;
    height:100%;
}
.min-nav .wrap ul{
    list-style:none;
    height:100%;
    position: absolute;
}
.min-nav .wrap ul li{
    float: left;
    display: inline-block;
    vertical-align: top;
    height: 38px;
    line-height: 38px;
    padding: 0 15px;
    font-size: 0.8em;
}
.wrap ul li:first-child {
    padding-left: 0;
}
.min-nav .wrap ul li a{
    text-decoration: none;
    color: #000;
}
.min-nav .wrap span{
    left:0;
    display: none;
    position: absolute;
    bottom:0;
    width:60%;
    height:3px;
    background-color: rgba(0,0,0,1);
}
.nav-content{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    z-index: 999;
    margin-top: 0;
    float: none;
    transform: translate(-100%,0);
}

.nav-content .nav-wrap{
    width: 100%;
    height: 50px;
    background: #fff;
    border-bottom: 1px solid #000;
    position: relative;
    z-index: 1;
}
.nav-content .nav-wrap .logo{
    width: 138px;
    display: block;
    position: absolute;
    top: 13px;
    left: 25px;
    cursor: pointer;
}
.nav-content .nav-wrap .logo img{
    width:100%;
}
.nav-content .nav-wrap i{
    position: absolute;
    top: 16px;
    right: 20px;
    background-position: -158px -125px;
    width: 16px;
    height: 16px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
}
.nav-content .scroll{
    background-color: white;
    width:90%;
}
.nav-content .scroll ul{
    list-style: none;
    background-color: white;
}
.nav-content .scroll ul li{
    display: block;
    width: 100%;
    margin: 0;
    height:52px;
    position: relative;
    border-top: 1px solid #d9d9d9;
    padding: 0 25px;
    line-height: 52px;
    color: #000;
    font-size: 12px;
}
.nav-content .scroll ul li .add{
    display: block;
    position: absolute;
    top: 20px;
    right: 25px;
    background-position: -89px -70px;
    width: 12px;
    height: 12px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
}
.nav-content .scroll ul li .angle{
    display: block;
    position: absolute;
    top: 20px;
    right: 25px;
    background-position: -90px -314px;
    width: 7px;
    height: 13px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
}
.nav-content .scroll ul li a{
    text-decoration: none;
    color: #000;
}
.nav-content .list{
    background: #f2f2f2;
    padding: 8px 0;
    width:90%;
}
.nav-content .list ul{
    list-style: none;
}
.nav-content .list ul li{
    display: block;
    line-height: 36px;
    padding: 0 25px;
    height:36px;
    font-size: 12px;
}
.nav-content .list ul li a{
    text-decoration: none;
    color: #000;
}
.nav-content .list ul li .angle{
    display: inline-block;
    position: relative;
    top: 2px;
    left:200px;
    background-position: -90px -314px;
    width: 7px;
    height: 13px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
}
.nav-content .list ul li .count{
     background-position: -38px -314px;
     width: 13px;
     height: 15px;
     display: inline-block;
     background-image: url(../images/icon@2x.png);
     background-size: 354px 354px;
     position: relative;
     top:2px;
     margin-right:18px;
 }
.nav-content .list ul li a .position{
    background-position: -20px -314px;
    width: 12px;
    height: 17px;
    display: inline-block;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    position: relative;
    top:2px;
    margin-right:18px;
}
.nav-content .list ul li .phone{
    background-position: -236px -279px;
    width: 15px;
    height: 15px;
    display: inline-block;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    position: relative;
    top:2px;
    margin-right:15px;
}
.nav-content .list ul li .problem{
    background-position: -74px -314px;
    width: 10px;
    height: 15px;
    display: inline-block;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    position: relative;
    top:2px;
    margin-right:18px;
}
.nav-content .scroll ul li:first-child{
    border-top:none;
}
section{
    padding-top: 145px;
}
.banner{
    width:100%;
    overflow: hidden;
    position: relative;
    font-size: 0;
    padding-bottom: 55px;
}
.banner ul{
    list-style: none;
    position: relative;
    width:700%;
    height:323px;
}
.banner ul li{
    float: left;
    width:14.285%;
}
.banner ul li img{
    width:100%;
    height:50%;
}
.banner .indicators{
    position: absolute;
    bottom:15px;
    padding:10px 0 30px;
    width:100%;
}
.banner .indicators ol{
    list-style: none;
    position: absolute;
    left:50%;
    transform: translateX(-50%);
    width:165px;
}
.banner .indicators ol li{
    float: left;
    margin: 0 9px;
    width: 15px;
    height: 15px;
    border: 1px solid #919191;
    background: #919191;
    border-radius: 50%;
}
.banner .indicators ol .active{
    background-color: black;
}
.common-header{
    width:100%;
    text-align: center;
}
.common-header p{
    letter-spacing: 2px;
    font-size:18px;
    font-weight: bold;
}
.aside{
    width:100%;
    max-width:640px;
}
.aside-nav{
    margin: 10px auto 15px;
    width:100%;
    border-bottom: 1px solid #e5e5e5;
    text-align: center;
}
.aside-nav ul{
    list-style: none;
    width: 316px;
    height:25px;
    margin:0 auto;
}
.aside-nav ul li{
    float: left;
}
.aside-nav ul li span{
    display: block;
    padding: 0 30px;
    height: 25px;
    font-size: 14px;
    letter-spacing: 2px;
    line-height: 25px;
    color: #a6a6a6;
    text-align: center;
    cursor: pointer;
}
.current{
    border-bottom: 2px solid #000;
}
.aside-nav ul li:first-child span{
    color: black;
}
.aside-content{
    position: relative;
    width:100%;
}
.aside-content  .swiper-container{
    margin-right: 30px;
    width: 100%;
    position: relative;
    overflow: hidden;
    z-index: 1;
}
.aside-content  .swiper-container .swiper-wrap{
    transform: translate3d(0px, 0px, 0px);
    width: 400%;
    position: relative;
    list-style: none;
}
.aside-content  .swiper-container .swiper-wrap li{
    float: left;
    height:auto;
}
.aside-content  .swiper-container .swiper-wrap li:nth-of-type(odd){
    width:12.5%;
    padding-right: 15px;
    padding-left: 30px;
    position: relative;
}
.aside-content  .swiper-container .swiper-wrap li:nth-of-type(even){
    padding-left: 15px;
    padding-right: 30px;
    width:12.5%;
    position: relative;
}
.aside-content  .swiper-container .swiper-wrap li .imgbox{
    position: relative;
    width:100%;
}
.aside-content  .swiper-container .swiper-wrap li .imgbox a img{
    width:100%;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox{
    text-align: center;
    padding-top: 60px;
    width:100%;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox h3 a{
    text-decoration: none;
    color: #000;
    font-size: 12px;
    line-height: 15px;
    font-weight: 700;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox p{
    margin-bottom: 6px;
    font-size: 12px;
    height: 30px;
    line-height: 15px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    white-space: normal;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox p a{
    color: #000;
    text-decoration: none;
    font-size: 12px;
    line-height: 15px;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .introduce a{
    color: #999;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .introduce{
    margin-bottom: 10px;
    font-size: 12px;
    line-height: 20px;
    height: 20px;
    white-space: nowrap;
    overflow: hidden;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .row{
    width:100%;
    height:auto;
    position: relative;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .row .star{
    margin-bottom: 15px;
    padding: 0;
    text-align: center;
    width:100%;
    position: relative;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .row .star i{
    background-position: -132px -208px;
    width: 77px;
    height: 13px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    position: absolute;
    left:50%;
    transform: translateX(-50%);
    margin-top: 13px;
    display: inline-block;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .row .price{
    text-align: center;
    line-height:14px;
    height:14px;
    width:100%;
    margin-bottom: 10px;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .addcart{
    width:100%;
    height:35px;
    background-color: #000;
    color: #fff;
    position: relative;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .addcart a{
    min-width: 96%;
    height: 35px;
    line-height: 35px;
    padding: 0 0 0 35px;
    font-size: 12px;
    letter-spacing: 1px;
    color: #fff;
    text-decoration: none;
}
.aside-content  .swiper-container .swiper-wrap li .detailbox .addcart a .bag{
    background-position: 0 -337px;
    width: 11px;
    height: 17px;
    position: absolute;
    left: 35px;
    top: 7px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    font-weight: 400;
}
.aside-content  .btn-right{
    width:20px;
    height:32px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    background-position: -305px -80px;
    font-weight: 400;
    position: absolute;
    top:0;
    right:0;
    top:40%;
    z-index: 999;
    display: block;
}
.aside-content  .btn-left{
    width:20px;
    height:32px;
    background-image: url(../images/icon@2x.png);
    background-size: 354px 354px;
    background-position: -305px -42px;
    font-weight: 400;
    position: absolute;
    top:0;
    left:0;
    top:40%;
    display: none;
    z-index: 999;
}
.aside-content .first{
    display: block;
}
.aside-content .second{
    display: none;
}
.aside-content .third{
    display: none;
}
.swiper-button{
    width:100%;
    padding: 25px 0 30px;
    margin-top: 0;
    bottom: 0;
    position: relative;
}
.swiper-button .active{
    background-color: #000;
    border-color: #000;
}
.swiper-button ul{
    list-style: none;
    width:132px;
    margin:0 auto;
}
.swiper-button ul li{
    width:15px;
    height:15px;
    border-radius: 50%;
    margin: 0 9px;
    border: 1px solid #919191;
    background: #919191;
    float: left;
}




.footer{
    border-top: 1px #000 solid;
    padding: 25px 0 0;
    width:100%;
}